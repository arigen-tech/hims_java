	<parameter name="balance_m_id" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select 
	store_balance_m.balance_m_id,
	concat(mas_hospital.hospital_name || '-' || mas_hospital.address) as hospital_name_address,
    store_balance_m.balance_no,
    mas_department.department_name,
    mas_store_item.pvms_no,
    mas_store_item.nomenclature,
    mas_store_unit.unit_name,
    mas_brand.brand_name,
    store_balance_t.batch_no,
    store_balance_t.expiry_date,
    store_balance_t.manufacture_date,
    store_balance_t.qty,
    store_balance_t.mrp_per_unit,
    store_balance_m.entered_dt as balance_dt,
    case upper(store_balance_m.status)
        when 'A' then 'Approved'
        when 'R' then 'Rejected'
		when 'P' then 'Pending'
		when 'S' then 'Saved'
        else ''
    end as status,
    store_balance_t.total_mrp_value as total_amount,
	store_balance_m.approved_by as approved_rejected_by,
	store_balance_m.remarks,
	store_balance_m.entered_by as submitted_by
from store_balance_t
left join store_balance_m on store_balance_t.balance_m_id = store_balance_m.balance_m_id
left join mas_department on store_balance_m.department_id = mas_department.department_id
left join mas_store_item on store_balance_t.item_id = mas_store_item.item_id
left join mas_store_unit on mas_store_item.unit_au = mas_store_unit.unit_id
left join mas_brand on store_balance_t.brand_id = mas_brand.brand_id
left join mas_hospital on store_balance_m.hospital_id = mas_hospital.hospital_id
where store_balance_m.balance_m_id = $P{balance_m_id}
order by mas_store_item.nomenclature]]>
	</queryString>
