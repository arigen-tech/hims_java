	<queryString>
		<![CDATA[select 
concat(mh.hospital_name || '-' || mh.address) as hospital_name_address,
md.department_name,
msi.pvms_no as mas_store_item_pvms_no,
msi.nomenclature as mas_store_item_nomenclature,
msu.unit_name,
sum(sibs.closing_stock) as balance_qty,
sibs.batch_no,
sibs.expiry_date,
sibs.manufacture_date, 
sibs.mrp_per_unit,
sibs.total_mrp_value,
mm.manufacturer_name,
mb.brand_name
from  store_item_batch_stock as sibs
inner join mas_store_item as msi on msi.item_id = sibs.item_id
left join mas_store_unit as msu on msi.unit_au = msu.unit_id
left join mas_department as md on sibs.department_id = md.department_id
left join mas_hospital as mh on sibs.hospital_id = mh.hospital_id
left join mas_manufacturer as mm on sibs.manufacturer_id = mm.manufacturer_id
left join mas_brand as mb on sibs.brand_id = mb.brand_id
where sibs.department_id = $P{DEPARTMENT_ID}
and sibs.hospital_id = $P{HOSPITAL_ID}
group by mh.hospital_name, md.department_name, msi.pvms_no, msi.nomenclature, 
		msu.unit_name, sibs.batch_no, sibs.expiry_date, sibs.manufacture_date, sibs.mrp_per_unit, mm.manufacturer_name, mb.brand_name, mh.address, sibs.total_mrp_value]]>
	</queryString>
