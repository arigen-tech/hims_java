	<queryString>
		<![CDATA[select 
concat(mas_hospital.hospital_name || '-' || mas_hospital.address) as hospital_name_address,
mas_department.department_name,
mas_store_item.pvms_no,
mas_store_item.nomenclature,
mas_store_unit.unit_name,
store_item_batch_stock.batch_no,
store_item_batch_stock.expiry_date,
sum(store_item_batch_stock.closing_stock) as closing_stock
from store_item_batch_stock 
left join mas_store_item on store_item_batch_stock.item_id = mas_store_item.item_id
left join mas_store_unit on mas_store_item.unit_au = mas_store_unit.unit_id
left join mas_department on store_item_batch_stock.department_id = mas_department.department_id
left join mas_hospital on store_item_batch_stock.hospital_id = mas_hospital.hospital_id
where (store_item_batch_stock.department_id = $P{DEPARTMENT_ID} or $P{DEPARTMENT_ID} = 0)
and (store_item_batch_stock.hospital_id = $P{HOSPITAL_ID} or $P{HOSPITAL_ID} = 0)
and store_item_batch_stock.expiry_date between $P{FromDate} and $P{ToDate}
group by
mas_hospital.hospital_name,
mas_hospital.address,
mas_department.department_name,
mas_store_item.pvms_no,
mas_store_item.nomenclature,
mas_store_unit.unit_name,
store_item_batch_stock.batch_no,
store_item_batch_stock.expiry_date,
store_item_batch_stock.closing_stock
order by
mas_hospital.hospital_name,
mas_store_item.nomenclature,
store_item_batch_stock.expiry_date]]>
	</queryString>
