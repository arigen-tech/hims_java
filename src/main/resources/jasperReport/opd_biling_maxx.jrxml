	<parameter name="visit_id" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select 
row_number() over(order by visit_no) as row_num, *
from
	(select
	distinct (bh.visit_id) as visit_no,
	bh.hospital_address as registered_office,
	p.p_mobile_number as patient_contact,
	bh.patient_display_name as patient_name,
	concat(bh.patient_age || '/' || bh.patient_gender) as age_sex,
	coalesce(concat(coalesce(p.p_address1, p.p_address2) || ',' || p.p_city || ',' || p.pincode), 'NA') as patient_address,
	--coalesce
	--	(case
	--	 	when p.p_address1 is not null then concat(p.p_address1 || ',' || p.p_city || ',' || p.pincode)
	--	 	when p.p_address2 is not null then concat(p.p_address2 || ',' || p.p_city || ',' || p.pincode)
	--	 	else 'NA'
	--	end, 'NA') as patient_address,
	coalesce(bh.referred_by, 'self') as referred_by,
	bh.billing_date as bill_date,
	bh.patient_id as patient_id,
	bh.bill_no,
	coalesce(bh.gstn_bill_no, 'NA') as gstn_bill,
	coalesce(bh.hospital_gstin, 'NA') as gstn_no,
	coalesce(mso.service_name, 'General Consultation') as opd_service_name,
	msc.sac_code,
	--work on qty later,
	(bh.total_amount + bh.discount_amount - bh.tax_total) as base_price,
	bh.discount_amount,
	bh.net_amount,
	bh.tax_total,
	bh.total_amount as bill_amount,
	concat('INR ' || bh.total_amount) as total,
	coalesce(bh.total_paid, 0) as total_paid,
	coalesce(pd.payment_mode, 'Cash') as payment_mode
	from billing_header as bh
	left join patient as p on bh.patient_id = p.patient_id
	left join billing_details as bd on bh.bill_hd_id = bd.bill_hd_id
	left join mas_service_opd as mso on bd.opd_service_id = mso.id
	left join mas_service_category as msc on bh.service_category_id = msc.id
	left join billing_payments as bp on bh.billing_hd_id = bp.billing_hd_id
	left join payment_details as pd on bp.payment_id = pd.payment_id)
where visit_no = $P{visit_id}]]>
	</queryString>
