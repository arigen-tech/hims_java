	<queryString>
		<![CDATA[select
	concat(mas_hospital.hospital_name || '-' || mas_hospital.address) as hospital_name_address,
	mas_department.department_name,
	mas_store_item.pvms_no as drug_code,
	mas_store_item.nomenclature as drug_name,
	mas_store_unit.unit_name,
	store_balance_t.batch_no,
	store_balance_t.expiry_date,
	store_balance_t.qty,
	store_balance_t.base_rate_per_unit as unit_rate,
	store_balance_t.total_mrp_value as total_amount,
	store_balance_m.entered_dt as balance_date,
	store_balance_m.approved_by
from store_balance_t
	left join store_balance_m on store_balance_m.balance_m_id = store_balance_t.balance_m_id
	left join mas_department on store_balance_m.department_id = mas_department.department_id
	left join mas_store_item on store_balance_t.item_id = mas_store_item.item_id
	left join mas_store_unit on mas_store_item.unit_au = mas_store_unit.unit_id
	left join mas_hospital on mas_store_item.hospital_id = mas_hospital.hospital_id	
where (store_balance_m.hospital_id = $P{HOSPITAL_ID} or $P{HOSPITAL_ID} = 0)
and (store_balance_m.entered_dt between $P{FromDate} and $P{ToDate})
and store_balance_m.department_id = $P{department_id} or ($P{department_id} = 0)
order by mas_store_item.nomenclature]]>
	</queryString>
