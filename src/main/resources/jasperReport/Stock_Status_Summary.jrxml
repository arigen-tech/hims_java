	<queryString>
		<![CDATA[select 
concat(mas_hospital.hospital_name || '-' || mas_hospital.address) as hospital_name_address,
mas_department.department_id,
mas_department.department_name,
mas_store_item.pvms_no as mas_store_item_pvms_no,
mas_store_item.nomenclature as mas_store_item_nomenclature,
mas_store_unit.unit_name,
sum(store_item_batch_stock.closing_stock) as balance_qty
from store_item_batch_stock
inner join mas_store_item on mas_store_item.item_id = store_item_batch_stock.item_id
left join mas_store_unit on mas_store_item.unit_au = mas_store_unit.unit_id
left join mas_department on store_item_batch_stock.department_id = mas_department.department_id
left join mas_hospital on store_item_batch_stock.hospital_id = mas_hospital.hospital_id
where store_item_batch_stock.department_id = $P{DEPARTMENT_ID}
and store_item_batch_stock.hospital_id = $P{HOSPITAL_ID}
and store_item_batch_stock.expiry_date > current_date
group by mas_hospital.hospital_name, mas_department.department_id, mas_store_item.pvms_no, mas_store_item.nomenclature, mas_store_unit.unit_name, mas_department.department_name, mas_hospital.address
order by mas_store_item.nomenclature]]>
	</queryString>
